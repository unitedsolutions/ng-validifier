{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "declaration": false,
    "allowJs": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": ["node", "jasmine", "jasminewd2"],
    "lib": ["es2015", "dom"]
  }
}
